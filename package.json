    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.15.0",
    "uninstall": "^0.0.0",
    "vite": "^5.4.14",
    "vite-plugin-dts": "^4.3.0",
    "yaml": "^2.6.1"
  }
